      <script src='//underscorejs.org/underscore-min.js' type='text/javascript'></script>
      </script>
    <![endif]-->
    <link rel="stylesheet" type="text/css" href="https://bootswatch.com/3/flatly/bootstrap.min.css">
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
